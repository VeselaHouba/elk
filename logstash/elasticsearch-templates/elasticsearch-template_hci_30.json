{
	"template": "logstash-*",
	"version": 1,
	"settings": {
		"index.refresh_interval": "5s",
		"number_of_shards": "10"
	},
	"mappings": {
		"_default_": {
			"_all": {
				"enabled": true,
				"norms": false
			},
			"dynamic_templates": [
				{
					"message_field": {
						"path_match": "message",
						"match_mapping_type": "string",
						"mapping": {
							"type": "text",
							"norms": false
						}
					}
				},
				{
					"string_fields": {
						"match_pattern": "regex",
						"match": ".*",
						"unmatch": "path|Unix_Hostname|Environment|Country|HCITRACE|worker|type|CorrelationId|Timestamp|ThreadId|Severity|Class|Hostname|Program|fingerprint|ManagedServer",
						"match_mapping_type": "string",
						"mapping": {
							"type": "text",
							"norms": false
						}
					}
				},
				{
					"path_mixed": {
						"match": "path",
						"match_mapping_type": "string",
						"mapping": {
							"type": "text",
							"norms": false,
							"fields": {
								"keyword": {
									"type": "keyword"
								}
							}
						}
					}
				},
				{
					"shared_keywords": {
						"match_pattern": "regex",
						"match": "Unix_Hostname|Environment|Country|HCITRACE|worker|type|CorrelationId|Timestamp|ThreadId|Severity|Class|Hostname|Program|fingerprint|ManagedServer",
						"match_mapping_type": "string",
						"mapping": {
							"type": "keyword"
						}
					}
				}
			],
			"properties": {
				"@timestamp": {
					"type": "date"
				},
				"@version": {
					"type": "keyword"
				},
				"geoip": {
					"dynamic": true,
					"properties": {
						"ip": {
							"type": "ip"
						},
						"location": {
							"type": "geo_point"
						},
						"latitude": {
							"type": "half_float"
						},
						"longitude": {
							"type": "half_float"
						}
					}
				}
			}
		},
		"wlserver": {
			"properties": {
				"MessageId": {
					"type": "keyword"
				},
				"DiagnosticContextId": {
					"type": "keyword"
				},
				"ThreadId": {
					"type": "keyword"
				},
				"Timestamp": {
					"type": "keyword"
				},
				"Severity": {
					"type": "keyword"
				},
				"UserId": {
					"type": "keyword"
				},
				"diag2": {
					"type": "keyword"
				}
			}
		},
		"wlsaccess": {
			"properties": {
				"Wla_TimeTakenToServeS": {
					"type": "float"
				},
				"Wla_RequestUri": {
					"type": "keyword"
				}
			}
		},
		"osblog": {
			"properties": {
				"Code": {
					"type": "keyword"
				},
				"ThreadId": {
					"type": "keyword"
				},
				"Timestamp": {
					"type": "keyword"
				},
				"Severity": {
					"type": "keyword"
				}
			}
		},
		"osbtrace": {
			"properties": {
				"Code": {
					"type": "keyword"
				},
				"ThreadId": {
					"type": "keyword"
				},
				"Timestamp": {
					"type": "keyword"
				},
				"Severity": {
					"type": "keyword"
				}
			}
		},
		"hcp_app": {
			"properties": {
				"Method": {
					"type": "keyword"
				},
				"RequestUri": {
					"type": "keyword"
				}
			}
		},
		"apache_access": {
			"properties": {
				"RemoteIp": {
					"type": "ip"
				},
				"RequestUri": {
					"type": "keyword"
				},
				"VirtualHost": {
					"type": "text",
					"fields": {
						"keyword": {
							"type": "keyword"
						}
					}
				},
				"JSESSIONID": {
					"type": "keyword"
				},
				"Timestamp": {
					"type": "keyword"
				},
				"RequestVersion": {
					"type": "keyword"
				},
				"geoip": {
					"dynamic": "true",
					"properties": {
						"timezone": {
							"type": "keyword"
						},
						"ip": {
							"type": "ip"
						},
						"latitude": {
							"type": "half_float"
						},
						"continent_code": {
							"type": "keyword"
						},
						"city_name": {
							"type": "keyword"
						},
						"country_code2": {
							"type": "keyword"
						},
						"country_name": {
							"type": "keyword"
						},
						"dma_code": {
							"type": "long"
						},
						"country_code3": {
							"type": "keyword"
						},
						"location": {
							"type": "geo_point"
						},
						"region_name": {
							"type": "keyword"
						},
						"postal_code": {
							"type": "keyword"
						},
						"longitude": {
							"type": "half_float"
						},
						"region_code": {
							"type": "keyword"
						}
					}
				}
			}
		},
		"apache_error": {
			"properties": {
				"Client": {
					"type": "ip"
				},
				"Timestamp": {
					"type": "keyword"
				}
			}
		},
		"jmserver": {
			"properties": {
				"Jms_Timestamp": {
					"type": "keyword"
				}
			}
		},
		"f5_access": {
			"properties": {
				"RequestUri": {
					"type": "keyword"
				},
				"VirtualHost": {
					"type": "text",
					"fields": {
						"keyword": {
							"type": "keyword"
						}
					}
				},
				"JSESSIONID": {
					"type": "keyword"
				},
				"TimeTakenToServeMs": {
					"type": "float"
				},
				"TimeTakenToServeUs": {
					"type": "float"
				},
				"StatusOriginal": {
					"type": "float"
				},
				"BytesReceived": {
					"type": "float"
				},
				"RequestVersion": {
					"type": "keyword"
				}
			}
		}
	}
}