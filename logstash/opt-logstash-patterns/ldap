LDAPREQ \[%{HTTPDATE:Apache_Timestamp}\] %{DATA:Ldap_Operation} %{DATA:Ldap_Type} conn=%{NUMBER:Ldap_Conn} op=%{NUMBER:Ldap_Op} msgID=%{NUMBER:Ldap_MsgID} %{GREEDYDATA:Ldap_Data}
LDAPRES \[%{HTTPDATE:Apache_Timestamp}\] %{DATA:Ldap_Operation} %{DATA:Ldap_Type} conn=%{NUMBER:Ldap_Conn} op=%{NUMBER:Ldap_Op} msgID=%{NUMBER:Ldap_MsgID} result=%{NUMBER:Ldap_Result} nentries=%{NUMBER:Ldap_Nentries} etime=%{NUMBER:Ldap_etime}
LDAPDCON \[%{HTTPDATE:Apache_Timestamp}\] %{DATA:Ldap_Operation} conn=%{NUMBER:Ldap_Conn} reason=\"%{DATA:Ldap_Reason}\"
LDAPCON \[%{HTTPDATE:Apache_Timestamp}\] %{DATA:Ldap_Operation} conn=%{NUMBER:Ldap_Conn} from=%{HOSTPORT:Ldap_From} to=%{HOSTPORT:Ldap_To} protocol=%{GREEDYDATA:Ldap_Protocol}
LDAPLOG %{LDAPRES}|%{LDAPCON}|%{LDAPDCON}|%{LDAPREQ}