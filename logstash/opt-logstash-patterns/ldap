LDAPREQ \[%{HTTPDATE:Ldap_Timestamp}\] %{DATA:Ldap_Operation} %{DATA:Ldap_Type} conn=%{NUMBER:Ldap_Conn:int} op=%{NUMBER:Ldap_Op:int} msgID=%{NUMBER:Ldap_MsgID:int}%{SPACE}%{GREEDYDATA:Ldap_Data}
LDAPRES \[%{HTTPDATE:Ldap_Timestamp}\] %{DATA:Ldap_Operation} %{DATA:Ldap_Type} conn=%{NUMBER:Ldap_Conn:int} op=%{NUMBER:Ldap_Op:int} msgID=%{NUMBER:Ldap_MsgID:int} result=%{NUMBER:Ldap_Result} nentries=%{NUMBER:Ldap_Nentries:int} etime=%{NUMBER:Ldap_etime:int}
LDAPDCON \[%{HTTPDATE:Ldap_Timestamp}\] %{DATA:Ldap_Operation} conn=%{NUMBER:Ldap_Conn:int} reason=\"%{DATA:Ldap_Reason}\"
LDAPCON \[%{HTTPDATE:Ldap_Timestamp}\] %{DATA:Ldap_Operation} conn=%{NUMBER:Ldap_Conn:int} from=%{HOSTPORT:Ldap_From} to=%{HOSTPORT:Ldap_To} protocol=%{GREEDYDATA:Ldap_Protocol}
LDAP_ACCESS_LOG %{LDAPRES}|%{LDAPCON}|%{LDAPDCON}|%{LDAPREQ}