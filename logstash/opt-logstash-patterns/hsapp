HSAPP2 (%{TIMESTAMP_ISO8601:TIMESTAMP}%{SPACE}\[(?<THREAD>.*)\]%{SPACE}(?<LEVEL>TRACE|DEBUG|INFO|WARN|ERROR|FATAL)\s+%{NOTSPACE:CLASS}%{SPACE}%{SPACE}%{GREEDYDATA:MESSAGE}(?<STACKTRACE>(.|\r|\n)*))
HSAPP3 (%{TIMESTAMP_ISO8601:TIMESTAMP}%{SPACE}(?<REST>(.|\r|\n)*))
HSAPP_PATH1 /var/log/fmw/app/%{DATA}/%{DATA:APP}/%{GREEDYDATA}
