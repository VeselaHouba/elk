APACHE_ACCESS_LOG1 (-|%{HOST:VirtualHost})(:%{POSINT})? \[remote\: %{DATA:RemoteHost} \(%{IP:RemoteIp}\)] \[user: %{DATA:RemoteUser} \(%{DATA:RemoteUse}\)] \[%{HTTPDATE:Timestamp}\] ([\\]?)\"(-|%{DATA:RequestMethod} %{DATA:RequestUri} %{DATA:RequestVersion})" (-|%{NUMBER:StatusOriginal:int}) \[size: (-|%{NUMBER:BytesReceived:int}), (-|%{NUMBER:BytesSent:int}), (-|%{NUMBER:BytesResponse:int})\] \[dur: (-|%{NUMBER:TimeTakenToServeMs:int}) us \((-|%{NUMBER:TimeTakenToServeS:int}) s\)\] \[SID=%{DATA:JSESSIONID}\] \[conn: %{DATA:CanonicalPortServing}, %{DATA:ConnectionStatusResponse}\] ([\\]?)"%{DATA:ContentType}" ([\\]?)"%{DATA:UserAgent}" "CALLID=%{DATA:CALLID}" "SOAPAction=([\\"]?)%{DATA:SOAPAction}([\\"]?)" "%{DATA:XRequestedWith}"( "HCITRACE=%{DATA:HCITRACE}")?
APACHE_ACCESS_LOG2 (-|%{HOST:VirtualHost})(:%{POSINT})? \[remote\: %{DATA:RemoteHost} \(%{IP:RemoteIp}\)] \[user: %{DATA:RemoteUser} \(%{DATA:RemoteUse}\)] \[%{HTTPDATE:Timestamp}\] ([\\]?)\"%{DATA:RequestMethod} %{DATA:RequestUri}( %{DATA:RequestVersion})?" (-|%{NUMBER:StatusOriginal:int}) \[size: (-|%{NUMBER:BytesReceived:int}), (-|%{NUMBER:BytesSent:int}), (-|%{NUMBER:BytesResponse:int})\] \[dur: (-|%{NUMBER:TimeTakenToServeMs:int}) us \((-|%{NUMBER:TimeTakenToServeS:int}) s\)\] \[SID=%{DATA:JSESSIONID}\] \[conn: %{DATA:CanonicalPortServing}, %{DATA:ConnectionStatusResponse}\] ([\\]?)"%{DATA:ContentType}" ([\\]?)"%{DATA:UserAgent}" ([\\]?)"%{DATA:XRequestedWith}"
