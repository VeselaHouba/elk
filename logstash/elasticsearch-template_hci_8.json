{
	"template" : "logstash-*",
	"settings" : {
		"index.refresh_interval" : "5s",
		"index.codec": "best_compression"
	},
	"mappings" : {
		"_default_": {
			"properties" : {
				"path": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				}
			}
		},
		"wlserver": {
			"properties" : {
				"Wls_MessageId": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				}
			}
		},
		"osbdiag": {
			"properties" : {
				"Osb_Code": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"Osb_ManagedServer": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				}
			}
		},
		"osblog": {
			"properties" : {
				"Osb_Code": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"Osb_ManagedServer": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				}
			}
		},
		"apache_access" : {
			"properties" : {
				"Apache_RemoteIp": { "type": "ip" },
				"Apache_RequestUri": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"Apache_VirtualHost": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"Apache_JSESSIONID": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"geoip"	: {
					"properties" : {
						 "coordinates" : { "type" : "geo_point", "doc_values" : true }
					}
				}
			}
		},
		"f5_access" : {
			"properties" : {
				"F5_RequestUri": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"F5_VirtualHost": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"F5_JSESSIONID": {
					"type" : "string",
					"fields" : {
						"raw" : {"type": "string", "index" : "not_analyzed", "doc_values" : true, "ignore_above" : 256}
					}
				},
				"geoip"	: {
					"properties" : {
						 "coordinates" : { "type" : "geo_point", "doc_values" : true }
					}
				}
			}
		}
	}
}
